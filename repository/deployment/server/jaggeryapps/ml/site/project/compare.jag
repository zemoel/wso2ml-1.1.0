    // compare function for classification models
    function sortByAccuracy(a, b) {
        var sortStatus = 0;
     
        if (a.modelSummary.modelAccuracy > b.modelSummary.modelAccuracy) {
            sortStatus = -1;
        } else if (a.modelSummary.modelAccuracy < b.modelSummary.modelAccuracy) {
            sortStatus = 1;
        }
        return sortStatus;
    }
    // compare function for numerical prediction models
    function sortByError(a, b) {
        var sortStatus = 0;
     
        if (a.modelSummary.meanSquaredError < b.modelSummary.meanSquaredError) {
            sortStatus = -1;
        } else if (a.modelSummary.meanSquaredError > b.modelSummary.meanSquaredError) {
            sortStatus = 1;
        }
        return sortStatus;
    }

   // compare function for numerical prediction models
    function sortByF1Score(a, b) {
        var sortStatus = 0;
        var percentileA = a.modelSummary.bestPercentile;
        var percentileB = b.modelSummary.bestPercentile;
     
        if (a.modelSummary.percentileToMulticlassMetricsMap[percentileA].f1Score > b.modelSummary.percentileToMulticlassMetricsMap[percentileB].f1Score) {
            sortStatus = -1;
        } else if (a.modelSummary.percentileToMulticlassMetricsMap[percentileA].f1Score < b.modelSummary.percentileToMulticlassMetricsMap[percentileB].f1Score) {
            sortStatus = 1;
        }
        return sortStatus;
    }

    // function to generate accordion panels
    function createAccordionPanels(models, modelType) {
        var accordionPanels = '';
        for (var i = 0; i < models.length; i++) {

            var modelId = sanitize(models[i].id);
            var modelName = sanitize(models[i].name);
            var algorithm = sanitize(models[i].modelSummary.algorithm.replace(/_/g, " "));
            var modelAccuracy = parseFloat(sanitize(models[i].modelSummary.modelAccuracy)).toFixed(2);
            var meanSquaredError = parseFloat(sanitize(models[i].modelSummary.meanSquaredError)).toExponential(2);
            if(sanitize(models[i].modelSummary.algorithm) == 'K_MEANS_ANOMALY_DETECTION_WITH_LABELED_DATA') {
                var percentile = parseFloat(sanitize(models[i].modelSummary.bestPercentile));
                var f1Score = parseFloat(sanitize(models[i].modelSummary.percentileToMulticlassMetricsMap[percentile].f1Score)).toFixed(4);                            
            }

            accordionPanels += '' + 
                '<div class="panel panel-default">' +
                    '<div class="panel-heading" role="tab" id="heading' + i + '">' +
                        '<h4 class="panel-title">' +
                            '<table class="tbl-projects" projectID="' + modelId + '">' +
                                '<tr>' +
                                    '<td>' +
                                        '<a data-parent="#' + modelType + '-accordion" href="#collapse' + i + '" aria-controls="collapse' + i + '" class="ctrl-exp-col reset-hover">' +
                                            '<span class="cu-acc-head-title">' + modelName + '</span>' +
                                            '<span class="cu-acc-head-workflow"></span>' +
                                            '<span class="cu-acc-head-created">[ Algorithm: ' + algorithm + ' ]</span>';
                                            if(modelType == 'classification') {
                                                accordionPanels += '<span class="cu-acc-head-created">[ Accuracy: <b>' + modelAccuracy + '%</b> ]</span>';
                                            }
                                            else if(modelType == 'numerical-prediction') {
                                                accordionPanels += '<span class="cu-acc-head-created">[ Mean squared error: <b>' + meanSquaredError + '</b> ]</span>';
                                            }
                                            else if(modelType == 'clustering') {
                                                accordionPanels += '<span class="cu-acc-head-created">[ Comparison not supported ]</span>';
                                            }
											else if(modelType == 'deeplearning') {
                                                accordionPanels += '<span class="cu-acc-head-created">[ Accuracy: <b>' + modelAccuracy + '%</b> ]</span>';
                                            }
                                            else if(modelType == 'anomaly-detection-labeled') {
                                                accordionPanels += '<span class="cu-acc-head-created">[ F1 score: <b>' + f1Score + '</b> ]</span>';
                                            }                                            
                                            else if(modelType == 'anomaly-detection-unlabeled') {
                                                accordionPanels += '<span class="cu-acc-head-created">[ Comparison not supported ]</span>';
                                            }                   
                                            else if(modelType == 'collaborative-filtering') {
                                                accordionPanels += '<span class="cu-acc-head-created">[ Mean squared error: <b>' + meanSquaredError + '</b> ]</span>';
                                            }                                                                        
                                    accordionPanels += '</a>' +
                                    '</td>' +
                                    '<td class="c-right">' +
                                        '<div>';
                                        if(modelType != 'anomaly-detection-unlabeled'){
                                            accordionPanels += '' + '<a href="#" id="view-model" class="cu-reg-btn btn-view" data-modelname="' + modelName + '" data-modelid="' + modelId + '" data-analysisid="' + models[i].analysisId + '" data-toggle="tooltip" data-original-title="View the model summary"><span>View</span></a>';
                                        }
                                        if(modelType != 'collaborative-filtering'){
                                           accordionPanels += '' + '<a href="#" id="predict" class="cu-reg-btn btn-predict" data-modelname="' + modelName + '" data-modelid="' + modelId + '" data-analysisid="' + models[i].analysisId +  '" data-toggle="tooltip" data-original-title="Predict using the model"><span>Predict</span></a>';
                                        }
                                        accordionPanels += '' +
                                        '<a href="#" id="download-model"  class="cu-reg-btn btn-download"  data-modelname="' + modelName + '" data-modelid="' + modelId +  '" data-analysisid="' + models[i].analysisId + '" data-toggle="tooltip" data-original-title="Download the model"><span>Download</span></a>' +
                                        '<a href="#" id="publish" class="cu-reg-btn btn-publish" data-modelid="' + modelId + '" data-analysisid="' + models[i].analysisId + '" data-toggle="tooltip" data-original-title="Publish the model to registry"><span>Publish</span></a>' +
                                        '</div>' +
                                    '</td>' +
                                '</tr>' +
                            '</table>' +
                        '</h4>' +
                    '</div>' +
                    '<div id="collapse' + i + '" class="panel-collapse collapse" role="tabpanel" aria-labelledby="heading' + i + '">' +
                        /*'<div class="panel-body">' + buildNotifications(models[i].error) + '</div>' +*/
                    '</div>' +
                '</div>';
        };
        return accordionPanels;
    }  

     $(document).on('click','#back-download-model, #back-publish-model',function(e){
            e.preventDefault();

            hidePopup();
        });

    // function to load models
    function loadModels() {        
        $.ajax({
            type: "GET",
            url: serverUrl+"/api/projects/" + projectId + "/models",
            async: false,
            success: function(data) {
                var projectModels = data;
                
                if (projectModels.length > 0) {

                    var classificationModels = [];
                    var numPredictionModels = [];
                    var clusteringModels = [];
					var deeplearningModels = [];
                    var anomalyDetectionModels = [];                    
                    var anomalyDetectionUnlabeledModels = [];                    
                    var collaborativeFilteringModels = [];                    

                    for (var i = 0; i < projectModels.length; i++) {
                        if(projectModels[i].modelSummary.algorithm == 'LOGISTIC_REGRESSION' || projectModels[i].modelSummary.algorithm == 'LOGISTIC_REGRESSION_LBFGS' || projectModels[i].modelSummary.algorithm == 'SVM' || projectModels[i].modelSummary.algorithm == 'DECISION_TREE' || projectModels[i].modelSummary.algorithm == 'RANDOM_FOREST_CLASSIFICATION' || projectModels[i].modelSummary.algorithm == 'NAIVE_BAYES') {
                            classificationModels.push(projectModels[i]);
                        }
                        else if(projectModels[i].modelSummary.algorithm == 'LINEAR_REGRESSION' || projectModels[i].modelSummary.algorithm == 'RIDGE_REGRESSION' || projectModels[i].modelSummary.algorithm == 'LASSO_REGRESSION' || projectModels[i].modelSummary.algorithm == 'RANDOM_FOREST_REGRESSION') {
                            numPredictionModels.push(projectModels[i]);
                        }
                        else if(projectModels[i].modelSummary.algorithm == 'K_MEANS') {
                            clusteringModels.push(projectModels[i]);
                        }
						else if(projectModels[i].modelSummary.algorithm == 'STACKED_AUTOENCODERS') {
                            deeplearningModels.push(projectModels[i]);
                        }
                        else if(projectModels[i].modelSummary.algorithm == 'K_MEANS_ANOMALY_DETECTION_WITH_LABELED_DATA') {
                            anomalyDetectionModels.push(projectModels[i]);
                        }                        
                        else if(projectModels[i].modelSummary.algorithm == 'K_MEANS_ANOMALY_DETECTION_WITH_UNLABELED_DATA') {
                            anomalyDetectionUnlabeledModels.push(projectModels[i]);
                        }     
                        else if(projectModels[i].modelSummary.algorithm == 'COLLABORATIVE_FILTERING' || projectModels[i].modelSummary.algorithm == 'COLLABORATIVE_FILTERING_IMPLICIT') {
                            collaborativeFilteringModels.push(projectModels[i]);
                        }                                                    
                    }

                    if (classificationModels.length > 0) {
                        var classificationModelsSorted = classificationModels.sort(sortByAccuracy);
                        var classificationAccordionPanels = createAccordionPanels(classificationModelsSorted, 'classification');
                        $('#no-classification').remove();                              
                        $("#classification-accordion").append(classificationAccordionPanels);
                        $('#classification-tab-li').removeClass('disabled');
                        $('#classification-tab-li').attr('title', '');
                        $('#classification-link').attr('data-toggle', 'tab');
                    }

                    if (numPredictionModels.length > 0) {
                        var numPredictionModelsSorted = numPredictionModels.sort(sortByError);
                        var numPredictionAccordionPanels = createAccordionPanels(numPredictionModelsSorted, 'numerical-prediction');
                        $('#no-numerical-prediction').remove();
                        $("#numerical-prediction-accordion").append(numPredictionAccordionPanels);
                        $('#numerical-prediction-tab-li').removeClass('disabled');
                        $('#numerical-prediction-tab-li').attr('title', '');
                        $('#numerical-prediction-link').attr('data-toggle', 'tab');
                    }

                    if (clusteringModels.length > 0) {
                        var clusteringAccordionPanels = createAccordionPanels(clusteringModels, 'clustering');
                        $('#no-clustering').remove();
                        $("#clustering-accordion").append(clusteringAccordionPanels);
                        $('#clustering-tab-li').removeClass('disabled');
                        $('#clustering-tab-li').attr('title', '');
                        $('#clustering-link').attr('data-toggle', 'tab');
                    }

					if (deeplearningModels.length > 0) {
                        var deeplearningModelsSorted = deeplearningModels.sort(sortByAccuracy);
                        var deeplearningAccordionPanels = createAccordionPanels(deeplearningModelsSorted, 'deeplearning');
                        $('#no-deeplearning').remove();                              
                        $("#deeplearning-accordion").append(deeplearningAccordionPanels);
                        $('#deeplearning-tab-li').removeClass('disabled');
                        $('#deeplearning-tab-li').attr('title', '');
                        $('#deeplearning-link').attr('data-toggle', 'tab');
                    }

                    if (anomalyDetectionModels.length > 0) {
                        var anomalyDetectionModelsSorted = anomalyDetectionModels.sort(sortByF1Score);
                        //var anomalyDetectionModelsSorted = anomalyDetectionModels;
                        var anomalyDetectionAccordionPanels = createAccordionPanels(anomalyDetectionModelsSorted, 'anomaly-detection-labeled');
                        $('#no-anomaly-detection-labeled').remove();                              
                        $("#anomaly-detection-labeled-accordion").append(anomalyDetectionAccordionPanels);
                        $('#anomaly-detection-labeled-tab-li').removeClass('disabled');
                        $('#anomaly-detection-labeled-tab-li').attr('title', '');
                        $('#anomaly-detection-labeled-link').attr('data-toggle', 'tab');
                    }   

                    if (anomalyDetectionUnlabeledModels.length > 0) {
                        //var anomalyDetectionModelsSorted = anomalyDetectionModels.sort(sortByAccuracy);
                        //var anomalyDetectionModelsSorted = anomalyDetectionUnlabeledModels;
                        var anomalyDetectionUnlabeledAccordionPanels = createAccordionPanels(anomalyDetectionUnlabeledModels, 'anomaly-detection-unlabeled');
                        $('#no-anomaly-detection-unlabeled').remove();                              
                        $("#anomaly-detection-unlabeled-accordion").append(anomalyDetectionUnlabeledAccordionPanels);
                        $('#anomaly-detection-unlabeled-tab-li').removeClass('disabled');
                        $('#anomaly-detection-unlabeled-tab-li').attr('title', '');
                        $('#anomaly-detection-unlabeled-link').attr('data-toggle', 'tab');
                    }                       

                    if (collaborativeFilteringModels.length > 0) {
                        var collaborativeFilteringModelsSorted = collaborativeFilteringModels.sort(sortByError);
                        var anomalyDetectionAccordionPanels = createAccordionPanels(collaborativeFilteringModelsSorted, 'collaborative-filtering');
                        $('#no-collaborative-filtering').remove();                              
                        $("#collaborative-filtering-accordion").append(anomalyDetectionAccordionPanels);
                        $('#collaborative-filtering-tab-li').removeClass('disabled');
                        $('#collaborative-filtering-tab-li').attr('title', '');
                        $('#collaborative-filtering-link').attr('data-toggle', 'tab');
                    }                                            
					
                    // set active tab
                    if(classificationModels.length > 0) {
                        $('#classification-link').click();
                    } else if(numPredictionModels.length > 0) {
                        $('#numerical-prediction-link').click();
                    } else if (clusteringModels.length > 0) {
                        $('#clustering-link').click();
                    } else if (deeplearningModels.length > 0) {
                        $('#deeplearning-link').click();
                    } else if (anomalyDetectionModels.length > 0) {
                        $('#anomaly-detection-labeled-link').click();
                    } else if (anomalyDetectionUnlabeledModels.length > 0) {
                        $('#anomaly-detection-unlabeled-link').click();  
                    } else if (collaborativeFilteringModels.length > 0) {
                        $('#collaborative-filtering-link').click();                                                 
                    } else {                        
                        $('#classification-link').click();
                    }
                }
            },
            error: function(res) {
                var errorText = res.responseText
                handleNotification(errorText, '#notification-area', 'warning');                
            }
        });

        bindEvents();            
    }
    // function to bind events with elements
    function bindEvents() {

        $(document).on('click', '#predict', function(e){ 
            e.preventDefault();
            
            var modelId = $(this).data('modelid');
            var modelName = $(this).data('modelname');
            var analysisId = $(this).data('analysisid');
            var analysisName = modelName.split(".")[0];
            window.location.href = '../predict/predict.jag?projectId='+projectId+'&analysisId='+analysisId+'&datasetId='+datasetId+'&modelId='+modelId+'&projectName='+projectName+'&analysisName='+analysisName+'&modelName='+modelName+'&fromCompare=true';
        });

        $(document).on('click', '#view-model', function(e){ 
            e.preventDefault();
            
            var modelId = $(this).data('modelid');
            var modelName = $(this).data('modelname');
            var analysisId = $(this).data('analysisid');
            var analysisName = modelName.split(".")[0];
            window.location.href = '../analysis/view-model.jag?projectId='+projectId+'&analysisId='+analysisId+'&datasetId='+datasetId+'&modelId='+modelId+'&projectName='+projectName+'&analysisName='+analysisName+'&modelName='+modelName+'&fromCompare=true';
        });

         var modelId ;
        $(document).on('click', '#publish', function(e){ 
            e.preventDefault();
            var analysisId = $(this).data('analysisid');
            var algorithmName = findAlgorithmName(analysisId); 
            if(loadPMMLAvailability(algorithmName)){
                    $('#publish-model-pmml').removeClass('btn-disabled').prop('title',"");
             } 
            modelId = $(this).data('modelid');
            operationSelect('publish-model');          
        });  
        $(document).on('click','#publish-model-serialized',function(e){
            e.preventDefault();
            $.ajax({
                type: "POST",
                url: serverUrl + "/api/models/" + modelId + "/publish?mode=serialized",
                dataType: 'json',
                success : function(res){
                    var successText = 'Model successfully published to registry location at: '+ sanitize(res["response"]);
                    handleNotification(successText, '#notification-area', 'success');
                },
                error : function(res){
                var errorText = JSON.parse(res.responseText)["exception"];
                handleNotification(errorText, '#notification-area', 'warning');
                }
            });
            
            hidePopup();
         });        
        $(document).on('click','#publish-model-pmml',function(e){
            e.preventDefault();
            $.ajax({
                type: "POST",
                url: serverUrl + "/api/models/" + modelId + "/publish?mode=pmml",
                dataType: 'json',
                success : function(res){
                    var successText = 'Model successfully published to registry location at: '+ sanitize(res["response"]);
                    handleNotification(successText, '#notification-area', 'success');
                },
                error : function(res){
                var errorText = JSON.parse(res.responseText)["exception"];
                handleNotification(errorText, '#notification-area', 'warning');
                }
            });
            
            hidePopup();
         }); 
        //events to handle downloading
        var modeld;
            $(document).on('click','#download-model',function(e){
                e.preventDefault();
                
                modelId = $(this).data('modelid');  
                var analysisId = $(this).data('analysisid');
                var algorithmName = findAlgorithmName(analysisId); 
                                   
                if(loadPMMLAvailability(algorithmName)){
                    $('#download-model-pmml').removeClass('btn-disabled').prop('title',"");
                }      
                operationSelect('download-model');
            });
            $(document).on('click','#download-model-serialized',function(e){
                e.preventDefault();

                window.location.href = serverUrl+'/api/models/'+modelId+'/export?mode=serialized';
                hidePopup();
            });
            $(document).on('click','#download-model-pmml',function(e){
                e.preventDefault();

                window.location.href = serverUrl+'/api/models/'+modelId+'/export?mode=pmml';
                hidePopup();
                
            });  


    }

    function loadPMMLAvailability(algorithmName){
            var status=false;
        if(algorithmName){
        
            $.ajax({
                type: "GET",
                url: serverUrl+'/api/configs/algorithms/'+algorithmName+'/exportable?format=pmml',
                async: false,
                success : function(res){
                    status = true;
                },
                error: function(res){
                    status = false;
                }
            });

           
        }
         return status;
    }

    function findAlgorithmName(analysisId){
            var algorithmName;
            $.ajax({
                type: "GET",
                url: serverUrl + "/api/analyses/" + sanitize(analysisId) + "/algorithmName",
                async : false,
                dataType: 'text',
                success : function(res){
                    algorithmName = sanitize(res);
                },
                error : function(res){
                var errorText = JSON.parse(res.responseText)["exception"];
                handleNotification(errorText, '#notification-area', 'warning');
                }
            });
            return algorithmName;
    }

</script>

